server:
  port: 8080

spring:
  datasource:
    url: jdbc:postgresql://localhost:5332/customers_db
    username: abd
    password: pwd
  jpa:
    hibernate:
      ddl-auto: create-drop
    properties:
      hibernate:
        dialect: org.hibernate.dialect.PostgreSQLDialect
        format-sql: true
      show-sql: true


  main:
    web-application-type: servlet